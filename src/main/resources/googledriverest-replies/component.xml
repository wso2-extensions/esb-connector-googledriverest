<component name="googledriverest-replies" type="synapse/template">
    <subComponents>
        <component name="listReplies">
            <file>listReplies.xml</file>
            <description>Lists all of the replies to a comment</description>
        </component>
        <component name="getReplies">
            <file>getReplies.xml</file>
            <description>Gets a reply</description>
        </component>
        <component name="insertReplies">
            <file>insertReplies.xml</file>
            <description>Creates a new reply to the given comment</description>
        </component>
        <component name="deleteReplies">
            <file>deleteReplies.xml</file>
            <description>Deletes a reply</description>
        </component>
    </subComponents>
</component>