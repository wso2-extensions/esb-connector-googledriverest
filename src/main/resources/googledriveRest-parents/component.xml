<component name="googledriveRest-parents" type="synapse/template">
    <subComponents>
        <component name="listParents">
            <file>listParents.xml</file>
            <description>Lists a file's parents</description>
        </component>
        <component name="getParent">
            <file>getParent.xml</file>
            <description>Gets a specific parent reference</description>
        </component>
        <component name="insertParent">
            <file>insertParent.xml</file>
            <description>Adds a parent folder for a file</description>
        </component>
        <component name="deleteParent">
            <file>deleteParent.xml</file>
            <description>Removes a parent from a file</description>
        </component>
    </subComponents>
</component>