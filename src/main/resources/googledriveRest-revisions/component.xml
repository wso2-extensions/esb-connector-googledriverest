<component name="googledriveRest-revisions" type="synapse/template">
    <subComponents>
        <component name="listRevisions">
            <file>listRevisions.xml</file>
            <description>Lists a file's revisions</description>
        </component>
        <component name="deleteRevisions">
            <file>deleteRevisions.xml</file>
            <description>Removes a revision</description>
        </component>
        <component name="getRevisions">
            <file>getRevisions.xml</file>
            <description>Gets a specific revision</description>
        </component>
        <component name="updateRevision">
            <file>updateRevision.xml</file>
            <description>Updates a revision</description>
        </component>
    </subComponents>
</component>