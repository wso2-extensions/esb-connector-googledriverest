<component name="googledriveRest-permissions" type="synapse/template">
    <subComponents>
        <component name="listPermissions">
            <file>listPermissions.xml</file>
            <description>List all permissions for a file</description>
        </component>
        <component name="getPermissions">
            <file>getPermissions.xml</file>
            <description>Gets a permission by ID</description>
        </component>
        <component name="insertPermission">
            <file>insertPermission.xml</file>
            <description>Inserts permission for a file</description>
        </component>
        <component name="deletePermissions">
            <file>deletePermissions.xml</file>
            <description>Deletes a permission from a file</description>
        </component>
        <component name="getIdForEmail">
            <file>getIdForEmail.xml</file>
            <description>Returns the permission ID for an email address</description>
        </component>
        <component name="updatePermission">
            <file>updatePermission.xml</file>
            <description>Updates permission for a file</description>
        </component>
    </subComponents>
</component>