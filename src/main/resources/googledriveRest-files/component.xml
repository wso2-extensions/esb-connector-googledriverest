<component name="googledriveRest-files" type="synapse/template">
    <subComponents>
        <component name="listFile">
            <file>listFile.xml</file>
            <description>Lists the authenticated user's files</description>
        </component>
        <component name="getFile">
            <file>getFile.xml</file>
            <description>Gets a file's metadata by ID</description>
        </component>
        <component name="copyFile">
            <file>copyFile.xml</file>
            <description>Creates a copy of the specified file</description>
        </component>
        <component name="insertFile">
            <file>insertFile.xml</file>
            <description>Inserts a new file metadata</description>
        </component>
        <component name="updateInsertFile">
            <file>updateInsertFile.xml</file>
            <description>Updates file metadata and content</description>
        </component>
        <component name="uploadFile">
            <file>uploadFile.xml</file>
            <description>Inserts a new file</description>
        </component>
        <component name="deleteFile">
            <file>deleteFile.xml</file>
            <description>Deletes a file</description>
        </component>
    </subComponents>
</component>