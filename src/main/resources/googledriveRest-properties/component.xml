<component name="googledriveRest-properties" type="synapse/template">
    <subComponents>
        <component name="listProperties">
            <file>listProperties.xml</file>
            <description>Lists a file's properties</description>
        </component>
        <component name="getProperties">
            <file>getProperties.xml</file>
            <description>Gets a property by its key</description>
        </component>
        <component name="insertProperties">
            <file>insertProperties.xml</file>
            <description>Adds a property to a file</description>
        </component>
        <component name="updateProperties">
            <file>updateProperties.xml</file>
            <description>Updates a property</description>
        </component>
        <component name="deleteProperties">
            <file>deleteProperties.xml</file>
            <description>Deletes a property</description>
        </component>
    </subComponents>
</component>