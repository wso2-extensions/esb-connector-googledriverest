<component name="googledriverest-children" type="synapse/template">
    <subComponents>
        <component name="listChildren">
            <file>listChildren.xml</file>
            <description>Lists a folder's Children</description>
        </component>
        <component name="getChild">
            <file>getChild.xml</file>
            <description>Gets a specific Children reference</description>
        </component>
        <component name="insertChild">
            <file>insertChild.xml</file>
            <description>Adds a Child file for a folder</description>
        </component>
        <component name="deleteChild">
            <file>deleteChild.xml</file>
            <description>Removes a Child from a folder</description>
        </component>
    </subComponents>
</component>